int loll(Node* head , int val)
{
    
    Node* temp = head;

    while (temp != NULL)
    {
        if(temp->data == val) return 1;
        temp = temp->next;
    }
    return 0;
}

int main()
{
    int key;
    cout << "Enter value to search: ";
    cin >> key;

    if (loll(head, key))
        cout << "Value found";
    else
        cout << "Value not found";

    return 0;
}
